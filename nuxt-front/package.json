    "shop:generate": "npm run shop:static && npm run shop:images && nuxt generate"
  },
  "dependencies": {
    "@nuxt/components": "^2.1.6",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/sitemap": "^2.4.0",
